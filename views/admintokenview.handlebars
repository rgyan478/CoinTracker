<div class="row" style="float:right">
    <a class="btn btn-secondar" href="/createtokenlist"><i class="fa fa-fw fa-lg fa-circle"></i>Add New TokenList</a>
</div>
<form id="form1" action="/update" method="POST">

<table class="table table-hover table-bordered" id="sampleTable">
    <input type="hidden"  name='id'  value=' {{#each data}}  {{_id}}  {{/each}}'>
    <thead >
        <tr style="text-align:center;padding:6px">
          
        <th>Token Code</th>
        <th>Token Name</th>
        <th>Edit</th>  
        <th>Delete</th>
      
               </tr>
    </thead>
    <tbody >
        {{#each data}}               
        <tr style="text-align:center;padding:6px">            
            <td  style="padding:6px;">{{tokencode}}</td>
            <td  style="padding:6px;">{{tokenname}} </td>            
            <td>
                 <a href='Edittokenlist/{{id}}'><i class="fa fa-pencil-square-o fa-2x" aria-hidden="true"></i></a></td> 
             <td> <a href='/deleted/{{_id}}' onclick='return confirm("Are you sure you want to delete?")'><i class="fa fa-trash fa-2x" aria-hidden="true"></i></a>
            </td>  
                                
        </tr>
        {{/each}}
     </tbody>
     <div></div>
</table>

</form>