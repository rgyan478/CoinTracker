   <div class="row token-list-main">
        <div class="col-md-8 col-xs-12">
          <div class="tile">
            <h3 class="tile-title">Token List</h3>
             {{#if error_msg}}
                <div class="alert alert-danger">{{error_msg}}</div>
              {{/if}}
            <div class="tile-body">
              <form class="form-horizontal" method="POST" action="/createtokenlist">
                <div class="form-group row">
                  <label class="control-label col-md-3">Token Code</label>
                  <div class="col-md-8">
                    <input class="form-control col-md-8" type="text" placeholder="Token Code" name="tokencode" style="text-transform:uppercase" required>
                  </div>
                </div>
                <div class="form-group row">
                  <label class="control-label col-md-3">Token Name</label>
                  <div class="col-md-8">
                    <input class="form-control col-md-8" type="text" placeholder="Token Name" name="tokenname" required>
                  </div>
                </div>
               
                </div>
                <div class="tile-footer">
                  <div class="row">
                    <div class="col-md-8 col-md-offset-3 col-xs-12">
                      <button class="btn btn-primary" type="submit"><i class="fa fa-fw fa-lg fa-check-circle"></i>Add</button>&nbsp;&nbsp;&nbsp;<a class="btn btn-secondary" href="/admintokenview"><i class="fa fa-fw fa-lg fa-check-circle"></i>Cancel</a>
                    </div>
                  </div>
                </div>
              </form>
            </div>

          </div>
        </div>
        <div class="col-md-6">
          {{!-- {{#if errors}}
              {{#each errors}}
                  <div class="alert alert-danger">{{msg}}</div>
              {{/each}}
          {{/if}} --}}
        </div>
    </div>